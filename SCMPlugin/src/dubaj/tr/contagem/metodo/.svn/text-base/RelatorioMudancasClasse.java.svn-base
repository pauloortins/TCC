package dubaj.tr.contagem.metodo;

public class RelatorioMudancasClasse {
	private String nomClasse;
	private int numMudancasClasse;
	private int numMudancasAtributo;
	private int numMudancasMetodo;
	private int numCodeChurn;
	private int numLinesOfCode;
	private double numCodeChurnRelativo;
	private int numCommits;
	
	public int getNumCommits() {
		return numCommits;
	}
	public String getNomClasse() {
		return nomClasse;
	}
	public void setNomClasse(String nomClasse) {
		this.nomClasse = nomClasse;
	}
	public int getNumMudancasClasse() {
		return numMudancasClasse;
	}
	public void setNumMudancasClasse(int numMudancasClasse) {
		this.numMudancasClasse = numMudancasClasse;
	}
	public int getNumMudancasAtributo() {
		return numMudancasAtributo;
	}
	public void setNumMudancasAtributo(int numMudancasAtributo) {
		this.numMudancasAtributo = numMudancasAtributo;
	}
	public int getNumMudancasMetodo() {
		return numMudancasMetodo;
	}
	
	public void setNumMudancasMetodo(int numMudancasMetodo) {
		this.numMudancasMetodo = numMudancasMetodo;
	}
	
	public int getNumMudancasTotal() {
		return numMudancasAtributo + numMudancasClasse + numMudancasMetodo;
	}
	
	public int getNumCodeChurn() {
		return numCodeChurn;
	}
	
	public void setNumCodeChurn(int numCodeChurn) {
		this.numCodeChurn = numCodeChurn;
	}
	
	public void addNumCommits()
	{
		this.numCommits++;
	}
	public int getNumLinesOfCode() {
		return numLinesOfCode;
	}
	public void setNumLinesOfCode(int numLinesOfCode) {
		this.numLinesOfCode = numLinesOfCode;
	}
	
	public double getNumCodeChurnRelativo() {
		return numCodeChurnRelativo;
	}
	public void setNumCodeChurnRelativo(double numCodeChurnRelativo) {
		this.numCodeChurnRelativo = numCodeChurnRelativo;
	}
	
	
}
